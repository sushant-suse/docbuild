[run]
concurrency = multiprocessing
parallel = true
branch = true
omit =
    tests/*

[report]
precision = 1
fail_under = 90.0
# Sort the report by missing coverage, descending
sort = -Cover
#
skip_covered = true
exclude_lines =
   pragma: no cover
   raise NotImplementedError
   if TYPE_CHECKING:
   if __name__ == .__main__.:
   if 0:
   if False:
